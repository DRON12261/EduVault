        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\andro\AppData\Roaming\Microsoft\UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
